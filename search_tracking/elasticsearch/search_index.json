{
  "settings": {
    "analysis": {
      "analyzer": {
        "csv_analyzer": {
          "tokenizer": "csv_tokenizer"
        }
      },
      "tokenizer": {
        "csv_tokenizer": {
          "type": "pattern",
          "pattern": ","
        }
      }
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "querystring_params": {
          "path_match": "query.*",
          "mapping": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword"
              },
              "csv": {
                "type": "text",
                "analyzer": "csv_analyzer"
              },
              "number": {
                "type": "long",
                "ignore_malformed": true
              }
            }
          }
        }
      },
      {
        "data_params": {
          "path_match": "data.*",
          "mapping": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword"
              },
              "csv": {
                "type": "text",
                "analyzer": "csv_analyzer"
              },
              "number": {
                "type": "long",
                "ignore_malformed": true
              }
            }
          }
        }
      }
    ],
    "properties": {
      "anonymousId": {
        "type": "keyword"
      },
      "event": {
        "type": "keyword"
      },
      "network": {
        "type": "keyword"
      },
      "@timestamp": {
        "type": "date"
      }
    }
  }
}
